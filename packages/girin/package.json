{
  "name": "girin",
  "version": "0.1.3",
  "description": "GraphQL framework for building flexible and maintainable schema",
  "author": "Max Choi <hanpama@gmail.com>",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p tsconfig.publish.json",
    "test": "jest"
  },
  "tags": [
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanpama/girin.git"
  },
  "bugs": {
    "url": "https://github.com/hanpama/girin/issues"
  },
  "homepage": "https://github.com/hanpama/girin",
  "license": "MIT",
  "devDependencies": {
    "@types/graphql": "^0.12.3",
    "@types/jest": "^22.1.1",
    "ts-jest": "^22.0.3",
    "graphql": "^0.12.3",
    "typescript": "2.6.2"
  },
  "peerDependencies": {
    "graphql": "^0.12.3"
  },
  "jest": {
    "mapCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/test/fixtures/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "jest": "^22.1.4"
  }
}
